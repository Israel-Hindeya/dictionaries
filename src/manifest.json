{
	"manifest_version": 2,
	"name": "Dictionaries",
	"permissions": ["activeTab", "tabs", "storage", "contextMenus", "<all_urls>"],
	"background": {
		"persistent": true,
		"page": "background.html"
	},
	"content_scripts": [
		{
			"js": ["inject.bundle.js"],
			"matches": ["<all_urls>"]
		}
	],
	"options_page": "options.html",
	"web_accessible_resources": ["*.js", "*.json", "*.html", "fonts/*"],
	"browser_action": {
		"default_icon": {
			"19": "images/books-96.png",
			"38": "images/books-96.png"
		}
	},
	"icons": {
		"48": "images/books-96.png"
	},
	"minimum_chrome_version": "23"
}
